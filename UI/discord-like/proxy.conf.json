{
  "/api/*": {
    "target": "http://localhost:4001/",
    "secure": false,
    "logLevel": "debug",
    "changeOrigin": true,
    "pathRewrite": { "^/api": "http://localhost:4001/api" }
  },

  "/ip/*": {
    "target": "https://api.ipify.org?format=json",
    "secure": false,
    "logLevel": "debug",
    "changeOrigin": true,
    "pathRewrite": { "^/ip": "" }
  }
}
